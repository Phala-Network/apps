{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["out/**", ".next/**", "build/**"],
      "env": ["CONTEXT", "NODE_ENV"]
    },
    "lint": {
      "outputs": [],
      "inputs": ["./**", "../../.prettierrc", "../../packages/eslint-config/**"]
    },
    "lint:fix": {
      "outputs": [],
      "inputs": ["./**", "../../.prettierrc", "../../packages/eslint-config/**"]
    },
    "check": {"outputs": []}
  }
}
